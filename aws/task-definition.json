{
    "family": "appwindows",
    "containerDefinitions": [
        {
            "image": "328527480917.dkr.ecr.us-east-1.amazonaws.com/appwindows:20",
            "name": "appwindows",
            "essential": true,
	    "cpu": 512,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp"
                }
             ]
        }
    ]
}

